{
  "compilerOptions": {
    "target": "ES2020",
    "outDir": "./dist",
    "rootDir": "./src",
    "module": "Node16",
    "types": ["node", "@vercel/node"],
    "moduleResolution": "node16",
    "noImplicitAny": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
